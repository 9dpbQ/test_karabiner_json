{
    "title": "Template_Combo",
    "rules": [
        {
            "description": "left arrow and right arrow -> Escape (Twitter)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "left_arrow"
                            },
                            {
                                "key_code": "right_arrow"
                            }
                        ],
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "escape"
                        }
                    ]
                }
            ]
        },
        {
            "description": "leftShift and rightShift -> Space (CSTimer)",
            "manipulators": [
                
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "left_shift"
                            },
                            {
                                "key_code": "right_shift"
                            }
                        ],
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "spacebar"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "right_command"
                            },
                            {
                                "key_code": "japanese_eisuu"
                            }
                        ],
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "spacebar"
                        }
                    ]
                }
            ]
        }
    ]
}